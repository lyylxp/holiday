        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
    <package name="loginManager" namespace="/login" extends="struts-default">
        <action name="loginAction" class="cn.edu.nwsuaf.action.LoginAction" method="login">
